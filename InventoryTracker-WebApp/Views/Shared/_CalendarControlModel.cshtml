            <div class="modal-body bodyStyle" id="calendarControlModelBody">
                <div class="container">
                    <!-- Popover #1 -->
                    @*<a class="btn btn-primary" data-html="true" data-placement="top" data-popover-content="#a1" data-toggle="popover" data-trigger="focus" href="#" tabindex="0">Popover Example</a>*@

                    <!-- Content for Popover #1 -->
                    <div class="hidden d-none" id="a1">
                        <div class="popover-heading">
                        </div>

                        <div class="popover-body">
                        </div>
                    </div>

                </div>
                <div class="row">
                    <div class="col-md-9">
                        <select class="form-select float-start selectDrpDown" aria-label="Equipment List">
                        </select>
                        <label id="ccEntityName" class="entityNameStyle" hidden></label>
                        <label id="ccEquipType" class="equipStyle" hidden></label>
                        <label id="ccVendor" class="equipStyle" hidden></label>
                        <label id="ccUnitID" class="equipStyle" hidden></label>
                        <div id="ccYearArrow" class="float-end">
                            <svg xmlns="http://www.w3.org/2000/svg" id="prevYear" width="25" height="25" fill="currentColor" class="bi bi-arrow-left-square arrowStyle" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M15 2a1 1 0 0 0-1-1H2a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2zM0 2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2zm11.5 5.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5z" />
