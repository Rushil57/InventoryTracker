@*Modal Start*@
<div class="modal fade" id="deleteAssignment" tabindex="-1" aria-labelledby="deleteAssignmentLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title" id="deleteAssignmentLabel">Assignment Options</h6>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" onclick="resetDeleteAssignmentModel()"></button>
            </div>
            <div class="modal-body" style="height:130px; overflow: auto;" id="entityTemplateModelBody">
                <div class="row">
                    <div class="col-5">
                        <h6>
                            <label>Start Date :</label>
                            <label type="text" style="width:100px;" readonly id="startDateLbl"></label>
                        </h6>
                    </div>
                    <div class="col-5">
                        <h6>
                            <label>End Date :</label> <label style="width: 100px;" type="text" readonly id="endDateLbl"></label>
                        </h6>
                    </div>
                </div>
                <div class="row">
                    <div class="col-5">
                        <h6>
                            <label>Start Date :</label><input type="text" class="updateStartDatepicker dropdown-control" style="display: inline; width: 60%; height:32px" readOnly />
                            </h6>
                    </div>
                    <div class="col-5">
                        <h6>
                            <label>End Date :</label><input type="text" class="updateEndDatepicker dropdown-control" style="display: inline; width:60%; height:32px" readOnly />
                        </h6>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" onclick="resetDeleteAssignmentModel()">Cancel</button>
                <button type="button" class="btn btn-primary" onclick="removeAssignmentOption()">Remove</button>
                <button type="button" class="btn btn-primary" onclick="updateAssignmentOption()">Update</button>

            </div>
        </div>
    </div>
</div>
@*Model End*@